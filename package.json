{
  "name": "feathers-massege",
  "description": "a fetherjs app",
  "version": "0.0.0",
  "homepage": "",
  "private": true,
  "keywords": [
    "feathers"
  ],
  "author": {
    "name": "romzyyy"
  },
  "contributors": [],
  "bugs": {},
  "engines": {
    "node": ">= 18.17.1"
  },
  "feathers": {
    "language": "ts",
    "packager": "pnpm",
    "database": "mongodb",
    "framework": "express",
    "transports": [
      "rest",
      "websockets"
    ],
    "schema": "typebox"
  },
  "directories": {
    "lib": "src",
    "test": "test"
  },
  "files": [
    "lib/client.js",
    "lib/**/*.d.ts",
    "lib/**/*.shared.js"
  ],
  "main": "lib/client",
  "scripts": {
    "dev": "nodemon -x ts-node src/index.ts",
    "compile": "shx rm -rf lib/ && tsc",
    "start": "node lib/",
    "prettier": "npx prettier \"**/*.ts\" --write",
    "mocha": "cross-env NODE_ENV=test mocha test/ --require ts-node/register --recursive --extension .ts --exit",
    "test": "npm run mocha",
    "bundle:client": "npm run compile && npm pack --pack-destination ./public"
  },
  "dependencies": {
    "@feathersjs/authentication": "^5.0.14",
    "@feathersjs/authentication-client": "^5.0.14",
    "@feathersjs/authentication-local": "^5.0.14",
    "@feathersjs/authentication-oauth": "^5.0.14",
    "@feathersjs/configuration": "^5.0.14",
    "@feathersjs/errors": "^5.0.14",
    "@feathersjs/express": "^5.0.14",
    "@feathersjs/feathers": "^5.0.14",
    "@feathersjs/mongodb": "^5.0.14",
    "@feathersjs/schema": "^5.0.14",
    "@feathersjs/socketio": "^5.0.14",
    "@feathersjs/transport-commons": "^5.0.14",
    "@feathersjs/typebox": "^5.0.14",
    "compression": "^1.7.4",
    "mongodb": "^6.3.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@feathersjs/adapter-commons": "^5.0.14",
    "@feathersjs/cli": "5.0.14",
    "@feathersjs/rest-client": "^5.0.14",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.10.6",
    "axios": "^1.6.3",
    "cross-env": "^7.0.3",
    "mocha": "^10.2.0",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.4",
    "shx": "^0.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}